generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model User {
  id          Int      @id @default(autoincrement())
  email       String   @unique
  nome        String
  senha       String
  dt_cadastro DateTime @default(now())
  animals     Animal[]
  @@map("users")
}
